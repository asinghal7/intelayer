<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>MyReportCompany</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
                <SVCURRENTCOMPANY>{{ company }}</SVCURRENTCOMPANY>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <REPORT NAME="MyReportCompany">
                        <FORMS>MyFormCompany</FORMS>
                    </REPORT>
                    <FORM NAME="MyFormCompany">
                        <PARTS>MyPartCompany</PARTS>
                    </FORM>
                    <PART NAME="MyPartCompany">
                        <LINES>MyLineCompany</LINES>
                        <REPEAT>MyLineCompany : MyCollCompany</REPEAT>
                        <SCROLLED>Vertical</SCROLLED>
                    </PART>
                    <LINE NAME="MyLineCompany">
                        <FIELDS>FldName, FldGUID, FldAddress, FldState, FldCountry, FldPincode, FldEmail</FIELDS>
                    </LINE>
                    <FIELD NAME="FldName"><SET>$Name</SET><XMLTAG>NAME</XMLTAG></FIELD>
                    <FIELD NAME="FldGUID"><SET>$GUID</SET><XMLTAG>GUID</XMLTAG></FIELD>
                    <FIELD NAME="FldAddress"><SET>$Address</SET><XMLTAG>ADDRESS</XMLTAG></FIELD>
                    <FIELD NAME="FldState"><SET>$StateName</SET><XMLTAG>STATENAME</XMLTAG></FIELD>
                    <FIELD NAME="FldCountry"><SET>$CountryName</SET><XMLTAG>COUNTRYNAME</XMLTAG></FIELD>
                    <FIELD NAME="FldPincode"><SET>$PinCode</SET><XMLTAG>PINCODE</XMLTAG></FIELD>
                    <FIELD NAME="FldEmail"><SET>$Email</SET><XMLTAG>EMAIL</XMLTAG></FIELD>
                    <COLLECTION NAME="MyCollCompany">
                        <TYPE>Company</TYPE>
                        <FILTER>FilterActiveCompany</FILTER>
                    </COLLECTION>
                    <SYSTEM TYPE="Formulae" NAME="FilterActiveCompany">$$IsEqual:##SVCurrentCompany:$Name</SYSTEM>
                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>
