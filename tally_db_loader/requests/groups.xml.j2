<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>MyReportGroups</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>$$SysName:XML</SVEXPORTFORMAT>
                <SVCURRENTCOMPANY>{{ company }}</SVCURRENTCOMPANY>
            </STATICVARIABLES>
            <TDL>
                <TDLMESSAGE>
                    <REPORT NAME="MyReportGroups">
                        <FORMS>MyFormGroups</FORMS>
                    </REPORT>
                    <FORM NAME="MyFormGroups">
                        <PARTS>MyPartGroups</PARTS>
                    </FORM>
                    <PART NAME="MyPartGroups">
                        <LINES>MyLineGroups</LINES>
                        <REPEAT>MyLineGroups : MyCollGroups</REPEAT>
                        <SCROLLED>Vertical</SCROLLED>
                    </PART>
                    <LINE NAME="MyLineGroups">
                        <FIELDS>FldName, FldGUID, FldParent, FldAlterId, FldIsRevenue, FldIsDeemedPositive, FldIsSubledger, FldAffectsGrossProfit</FIELDS>
                    </LINE>
                    <FIELD NAME="FldName"><SET>$Name</SET><XMLTAG>NAME</XMLTAG></FIELD>
                    <FIELD NAME="FldGUID"><SET>$GUID</SET><XMLTAG>GUID</XMLTAG></FIELD>
                    <FIELD NAME="FldParent"><SET>$Parent</SET><XMLTAG>PARENT</XMLTAG></FIELD>
                    <FIELD NAME="FldAlterId"><SET>if $$IsEmpty:$AlterId then "0" else $$String:$AlterId</SET><XMLTAG>ALTERID</XMLTAG></FIELD>
                    <FIELD NAME="FldIsRevenue"><SET>if $IsRevenue then "Yes" else "No"</SET><XMLTAG>ISREVENUE</XMLTAG></FIELD>
                    <FIELD NAME="FldIsDeemedPositive"><SET>if $IsDeemedPositive then "Yes" else "No"</SET><XMLTAG>ISDEEMEDPOSITIVE</XMLTAG></FIELD>
                    <FIELD NAME="FldIsSubledger"><SET>if $IsSubLedger then "Yes" else "No"</SET><XMLTAG>ISSUBLEDGER</XMLTAG></FIELD>
                    <FIELD NAME="FldAffectsGrossProfit"><SET>if $AffectsGrossProfit then "Yes" else "No"</SET><XMLTAG>AFFECTSGROSSPROFIT</XMLTAG></FIELD>
                    <COLLECTION NAME="MyCollGroups">
                        <TYPE>Group</TYPE>
                    </COLLECTION>
                </TDLMESSAGE>
            </TDL>
        </DESC>
    </BODY>
</ENVELOPE>
